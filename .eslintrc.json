{
    "env": {
      "es6": true,
      "node": true,
      "mocha": true,
      "browser": true
    },
    "extends": ["eslint:recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "error",
      "quotes": [2, "single"],
      "prettier/prettier": "error"
    }
}
